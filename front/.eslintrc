{
	"parser": "babel-eslint",
	"parserOptions": {
		"ecmaVersion": 2018,
		"sourceType": "module",
		"ecmaFeatures": {
			"jsx": true
		}
	},
	"env": {
		"browser": true,
		"node": true,
		"es6": true
	},
	"plugins": [
		"react-hooks",
		"import"
	],
	"extends": [
    "plugin:jsx-a11y/recommended"
  ]
}	